	<ThingDef ParentName="VNPE_NutrientPasteFridgeFeeder">
		<defName>VNPE_NutrientPasteFridgeFeeder0T</defName>
		<label>nutrient paste feeder</label>
		<description>A small pipe output that allows colonists and citizens to pick up nutrient paste.</description>
		<graphicData>
			<texPath>Things/Building/FridgeFeeder/NutrientPasteFridgeFeederT0</texPath>
			<color>(232,255,191)</color>
		</graphicData>
		<statBases>
			<WorkToBuild>1200</WorkToBuild>
		</statBases>
		<costList>
			<Steel>25</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<comps Inherit="False">
			<li Class="PipeSystem.CompProperties_ConvertResourceToThing">
				<pipeNet>VNPE_NutrientPasteNet</pipeNet>
				<maxOutputStackSize>5</maxOutputStackSize>
				<thing>MealNutrientPaste</thing>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>70</basePowerConsumption>
			</li>
		</comps>
		<modExtensions>
			<li Class="VNPEReimaginedProgression.VNPERPDefModExtension">
				<customMeal>MealNutrientPasteDT</customMeal>
				<storageCost>0.66667</storageCost>
			</li>
		</modExtensions>
	</ThingDef>
	
